{
  "name": "mp-i18n",
  "version": "2.2.1",
  "description": "A cross-platform i18n library for muti-miniprograms (taro、wx、alipay、baidu、tt).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm pack"
  },
  "keywords": [
    "miniprogram",
    "i18n",
    "taro",
    "wxapp",
    "tt",
    "swan",
    "my"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/mp-i18n.git"
  },
  "devDependencies": {
    "bundle-dts": "^1.1.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
